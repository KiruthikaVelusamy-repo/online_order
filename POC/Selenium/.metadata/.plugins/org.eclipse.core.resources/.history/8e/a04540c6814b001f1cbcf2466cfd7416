	{
		
		landingPage.signIn("Kiruthikavelusamy.in@gmail.com", "Test@123");
		
		Assert.assertEquals(landingPage.errorMessage, "Wrong or Invalid e-mail address or mobile phone number. Please correct and try again");
	}
}
